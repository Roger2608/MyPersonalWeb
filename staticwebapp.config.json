{
    "routes": [
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*",
            "/css/*"
        ]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html"
        }
    }
}